<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oopbank.FXMLChangeCustomerInfoController">
   <children>
      <VBox spacing="10.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Label text="Change customer info">
                     <font>
                        <Font size="30.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
            <GridPane prefHeight="120.0" prefWidth="147.0" vgap="20.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="87.0" minWidth="10.0" prefWidth="50.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="168.0" minWidth="10.0" prefWidth="168.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Last name" GridPane.rowIndex="1" />
                  <Label text="SSN" GridPane.rowIndex="2" />
                  <TextField fx:id="txtFieldLastName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="lblSSN" text="888888-8888" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="First name" />
                  <TextField fx:id="txtFieldFirstName" GridPane.columnIndex="1" />
               </children>
               <VBox.margin>
                  <Insets left="30.0" right="30.0" />
               </VBox.margin>
            </GridPane>
            <HBox>
               <children>
                  <Label fx:id="lblAlert" textFill="RED" />
               </children>
               <VBox.margin>
                  <Insets left="30.0" right="30.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Button fx:id="btnSaveChanges" mnemonicParsing="false" onAction="#clickedSaveChanges" text="Save changes" />
                  <Button fx:id="btnClose" mnemonicParsing="false" onAction="#btnCloseClicked" text="Close" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="30.0" right="30.0" top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
